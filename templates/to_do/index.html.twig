{% extends 'template.html.twig' %}

{% block body %}
    {% block PageTitle %}
        To Do List
    {% endblock %}
    
    <ol class="list-group">
        {% for name,content in app.session.get('todo') %}
            <li class="list-group-item">
                {{ name }} : {{ content }}
            </li>
        {% endfor %}
    </ol>
{% endblock %}

{% block title %}
    To Do List
{% endblock %}
